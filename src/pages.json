{
  "globalStyle": {
    "navigationStyle": "custom"
  },
  "pages": [
    {
      "path": "pages/router/router",
      "style": {
        "disableScroll": true
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-stopwatch",
      "pages": [
        {
          "path": "main/main",
          "style": {
            "disableScroll": true
          }
        },
        {
          "path": "trash/trash",
          "style": {
            "disableScroll": true
          }
        },
        {
          "path": "tags/tags",
          "style": {
            "disableScroll": true
          }
        },
        {
          "path": "tagsview/tagsview",
          "style": {
            "disableScroll": true
          }
        }
      ]
    },
    {
      "root": "pages-timer",
      "pages": [
        {
          "path": "main/main",
          "style": {
            "disableScroll": true
          }
        }
      ]
    },
    {
      "root": "pages-clock",
      "pages": [
        {
          "path": "main/main",
          "style": {
            "disableScroll": true,
            "mp-weixin": {
              "pageOrientation": "landscape",
              "renderer": "webview"
            }
          }
        }
      ]
    }
  ]
}
